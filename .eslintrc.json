{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended", 
        "plugin:@typescript-eslint/eslint-recommended",
        "plugin:@typescript-eslint/recommended",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "rules": {
        "no-console": 1,       // Means warning
    "prettier/prettier": 2 // Means error
    }
}
